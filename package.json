{
  "name": "node-mentoring",
  "version": "1.0.0",
  "description": "node mentoring test app",
  "main": "src/app.js",
  "scripts": {
    "start": "nodemon --exec babel-node src/app.js",
    "start:sync": "IMPORT_MODE=sync nodemon --exec babel-node src/app.js",
    "build": "babel src --out-dir dist",
    "serve": "node dist/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate": "node csv-generator.js > data/new-test-file.csv",
    "streams": "babel src/utils/streams.js --out-dir dist/utils && node dist/utils/streams.js"
  },
  "author": "Nick Osiunin <one.design@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/register": "^7.0.0",
    "csv": "^5.0.1",
    "express": "^4.16.4",
    "md5": "^2.2.1",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "csvtojson": "^2.0.8",
    "minimist": "^1.2.0",
    "through2": "^3.0.0"
  }
}
