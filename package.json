{
  "name": "node-mentoring",
  "version": "1.0.0",
  "description": "node mentoring test app",
  "main": "src/app.js",
  "scripts": {
    "serve": "nodemon --exec babel-node src/index.js",
    "db:stop-previous": "docker stop task7mongo || true",
    "db:docker": "docker container run -d -p 27017:27017 --name task7mongo --rm -e MONGO_INITDB_ROOT_PASSWORD=admin -e MONGO_INITDB_ROOT_USERNAME=admin -e MONGO_INITDB_DATABASE=task7 mongo:3.4.20-jessie && sleep 5",
    "db:seed": "cd src && mongoose seed",
    "dbprepare": "run-s db:*",
    "task7": "run-s dbprepare serve"
  },
  "author": "Nick Osiunin <one.design@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/register": "^7.4.0",
    "express": "^4.16.4",
    "md5": "^2.2.1",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.2.3",
    "mongoose": "^5.5.0",
    "mongoose-model-cli": "^1.4.0"
  }
}
