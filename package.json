{
  "name": "node-mentoring",
  "version": "1.0.0",
  "description": "node mentoring test app",
  "main": "src/app.js",
  "scripts": {
    "start": "nodemon --exec babel-node src/app.js",
    "start:sync": "IMPORT_MODE=sync nodemon --exec babel-node src/app.js",
    "build": "babel src --out-dir dist",
    "serve": "node dist/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate": "node csv-generator.js > data/new-test-file.csv",
    "streams": "babel src/utils/streams.js --out-dir dist/utils && node dist/utils/streams.js",
    "task6:serve": "nodemon --exec babel-node src/http-servers/app/index.js",
    "task6:docker": "docker container run -d -p 5432:5432 --name task6postgress --rm -e POSTGRES_PASSWORD=admin -e POSTGRES_USER=admin -e POSTGRES_DB=task6 postgres:11.2-alpine",
    "task6:migrate": "cd src/http-servers/app/db && npx sequelize db:migrate"
  },
  "author": "Nick Osiunin <one.design@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/register": "^7.0.0",
    "csv": "^5.0.1",
    "express": "^4.16.4",
    "md5": "^2.2.1",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "csvtojson": "^2.0.8",
    "jsonwebtoken": "^8.4.0",
    "minimist": "^1.2.0",
    "pg": "^7.8.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.43.0",
    "sequelize-cli": "^5.4.0",
    "through2": "^3.0.0"
  }
}
